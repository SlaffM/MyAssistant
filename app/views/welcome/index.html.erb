<h1>Hello, Rails!</h1>

<% if user_signed_in? %>
  <h3><%= link_to 'My Articles', controller: 'articles' %></h3>
  <h3><%= link_to 'My Shedules', controller: 'shedules' %></h3>
  <p><%= @shedule_type %></p>
  <p><%= @images %></p>

  <% @images.each do |image_url| %>
